{
  "name": "b2c",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prestart": "bun upgrade",
    "start": "bun run dev",
    "dev": "concurrently --kill-others \"bun styles.ts --watch\" \"bun states.ts --watch\" \"cross-env scriptOnly=true vite build --mode development --watch\" \"bun server.ts --mode development\"",
    "serve": "bun run dev",
    "watch": "bun server.ts",
    "build": "bun run build:static && bun run build:server",
    "build:eshn": "bun run build:static:eshn && bun run build:server:eshn",
    "preview": "vite preview",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:server:eshn": "vite build --ssr src/entry-server.tsx --outDir dist/server --mode eshn",
    "build:static": "vite build --outDir dist/static",
    "build:static:eshn": "vite build --outDir dist/static --mode eshn",
    "generate": "bun states.ts && bun run styles && bun run build && bun prerender.ts --add-hash",
    "eshn": "bun states.ts && bun run styles && bun run build:eshn && bun prerender.ts --add-hash --mode eshn && bun run build:web-components",
    "inte": "bun run styles && bun run build && bun prerender.ts && bun run build:web-components && bun integration.ts",
    "styles": "bun styles.ts",
    "format": "prettier --write .",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "generate:api": "orval --config ./orval.config.ts",
    "build:web-components": "vite build --config src/web-components/vite.config.ts"
  },
  "dependencies": {
    "msw": "^2.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/css": "^0.0.37",
    "@types/debounce": "^1.2.4",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/js-beautify": "^1.14.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css": "^3.0.0",
    "cssnano": "^6.0.1",
    "debounce": "^2.0.0",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "js-beautify": "^1.14.11",
    "lodash": "^4.17.21",
    "orval": "^6.28.2",
    "postcss": "^8.4.32",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.1.1",
    "rollup": "^4.8.0",
    "sass": "^1.69.5",
    "slash": "^5.1.0",
    "svgo": "^3.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
